#include<bits/stdc++.h>
using namespace std;

class FoxAndGame{
    public:
    int countStars(vector<string> input){
        int cnt=0;
        int siz=input.size();
        for(int i=0;i<siz;i++){
            for(int j=0;j<3;j++){
                if(input[i][j]=='o') cnt++;
            }
        }
        
        return cnt;
    }
};
